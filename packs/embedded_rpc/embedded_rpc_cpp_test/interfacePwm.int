enum ChannelId
  ChannelLow
  ChannelHigh
end

interface PWM
  # Events
  EVENT PwmUpdate(ChannelId channel, uint16_t value)

  # Functions
  void SetPwm(ChannelId channel, uint16_t value) = 100 # second
  uint16_t GetPwm(ChannelId channel)
  void ResetPwm()

  void SetName(PCString name)
  PCString GetName()
end